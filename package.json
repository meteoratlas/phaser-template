{
  "name": "phaser-template",
  "version": "1.0.0",
  "description": "Quick start template for Phaser, with Webpack",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --mode development --entry ./src/entry.js",
    "build": "webpack --mode production --entry ./src/entry.js --output ./dist/entry.js",
    "dev": "webpack --mode development --entry ./src/entry.js --output ./dist/entry.js"
  },
  "author": "Kelly Horan",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.6.0",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.5.3",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "dependencies": {
    "phaser": "^3.55.2"
  }
}
